{
  "name": "alexa-poe-helper",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && gulp",
    "deploy": "npm run build && ask deploy",
    "deploy:lambda": "npm run build && ask deploy --target lambda",
    "deploy:local": "ask deploy --target skill --profile local",
    "start": "nodemon",
    "generate-item-names": "ts-node lambda/tools/generateItemNames.ts",
    "test": "jest",
    "start:test": "nodemon --config nodemon_tests.json"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testMatch": [
      "**/*.spec.ts"
    ]
  },
  "author": "Mihail Cristian Dumitru",
  "license": "MIT",
  "dependencies": {
    "ask-sdk-core": "^2.0.7",
    "ask-sdk-model": "^1.3.1",
    "i18next": "^11.3.5",
    "i18next-sprintf-postprocessor": "^0.2.2",
    "request": "^2.87.0"
  },
  "devDependencies": {
    "@types/node": "^10.5.2",
    "@types/express": "^4.16.0",
    "@types/i18next": "^8.4.3",
    "@types/i18next-sprintf-postprocessor": "^0.0.29",
    "@types/request": "2.47.0",
    "@types/jest": "^23.3.0",
    "@types/lodash": "^4.14.112",
    "express": "^4.16.3",
    "gulp": "^3.9.1",
    "gulp-typescript": "^4.0.1",
    "rimraf": "^2.6.2",
    "ts-node": "^5.0.1",
    "typescript": "^2.9.2",
    "nodemon": "^1.17.5",
    "jest": "^23.4.1",
    "ts-jest": "^23.0.1",
    "lodash": "^4.17.10"
  }
}
